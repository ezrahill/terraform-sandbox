variable "prefix" {
  description = "Naming Prefix"
  type        = string
  default     = "acg"
}

variable "env" {
  description = "Name of Environment"
  type        = string
  default     = "dev"
}

variable "region" {
  description = "Name of AWS Region"
  type        = string
  default     = "us-east-1"
}

